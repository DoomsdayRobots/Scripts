echo "*******************************************************************"
echo " "

echo "Downloading: ChromaV5%20(2.0)"
wget https://huggingface.co/SomaMythos/ChromaV5/resolve/main/ChromaV5%20(2.0).ckpt

echo "Downloading: ChromaV5"
wget https://huggingface.co/SomaMythos/ChromaV5/resolve/main/ChromaV5.ckpt

