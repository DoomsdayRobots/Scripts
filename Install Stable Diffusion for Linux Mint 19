echo "lets go to our folder 'Stable_Diffusion' and then to our folder 'stable-diffusion-webui'."  
cd ~/Stable_Diffusion/stable-diffusion-webui/

echo "lets add this line of text 'git pull' to our 'webui-user.bat' file."
echo ' ' | cat - webui-user.bat > temp && mv temp webui-user.bat
echo 'git pull ' | cat - webui-user.bat > temp && mv temp webui-user.bat

#echo "Downloading: "
#wget

